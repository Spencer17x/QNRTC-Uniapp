{
    "name": "QNRtcUniPlugin",
    "id": "QNRtcUniPlugin",
    "version": "1.0.0",
    "description": "QNRTC原生插件",
    "_dp_type": "nativeplugin",
    "_dp_nativeplugin": {
        "ios": {
            "plugins": [
                {
                    "type": "module",
                    "name": "QNRtcUniPlugin-QNRtcEngine",
                    "class": "QNRtcEngine"
                },
                {
                    "type": "module",
                    "name": "QNRtcUniPlugin-QNRtcClient",
                    "class": "QNRtcClient"
                },
                {
                    "type": "module",
                    "name": "QNRtcUniPlugin-QNRtcTrack",
                    "class": "QNRtcTrack"
                },
                {
                    "type": "component",
                    "name": "surfaceView",
                    "class": "QNRtcSurfaceView"
                }
            ],
            "integrateType": "framework",
            "frameworks": [
                "HappyDNS.framework",
                "QNRTCKit.framework"
            ],
            "embedFrameworks": [
                "HappyDNS.framework",
                "QNRTCKit.framework"
            ],
            "privacies": [
                "NSCameraUsageDescription",
                "NSMicrophoneUsageDescription"
            ],
            "deploymentTarget": "9.0"
        },
        "android": {
            "plugins": [
                {
                    "type": "module",
                    "name": "QNRtcUniPlugin-QNRtcEngine",
                    "class": "uni.qiniu.droid.uniplugin_rtc.uni.module.QNRTCModule"
                },
                {
                    "type": "module",
                    "name": "QNRtcUniPlugin-QNRtcClient",
                    "class": "uni.qiniu.droid.uniplugin_rtc.uni.module.QNRTCClientModule"
                },
                {
                    "type": "module",
                    "name": "QNRtcUniPlugin-QNRtcTrack",
                    "class": "uni.qiniu.droid.uniplugin_rtc.uni.module.QNRTCTrackModule"
                },
                {
                    "type": "component",
                    "name": "surfaceView",
                    "class": "uni.qiniu.droid.uniplugin_rtc.uni.component.QNRTCSurfaceView"
                },
		{
		    "type": "component",
		    "name": "textureView",
		    "class": "uni.qiniu.droid.uniplugin_rtc.uni.component.QNRTCTextureView"
		}
            ],
            "abis": [
                "armeabi-v7a",
                "x86",
                "arm64-v8a"
            ],
	    "dependencies": [
		"com.qiniu:happy-dns:0.2.18",
                "com.google.code.gson:gson:2.8.8"
	    ],
            "minSdkVersion": "18",
            "compileOptions": {
                "sourceCompatibility": "1.8",
                "targetCompatibility": "1.8"
            },
            "integrateType": "aar"
        }
    }
}
